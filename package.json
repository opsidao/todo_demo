{
  "name": "todo_demo",
  "private": true,
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "jest": "jest"
  },
  "dependencies": {
    "@material-ui/core": "^1.3.1",
    "@rails/webpacker": "3.5",
    "axios": "^0.18.0",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-transform-regenerator": "^6.26.0",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "connected-react-router": "^4.3.0",
    "cypress": "^3.0.2",
    "react": "^16.4.1",
    "react-dom": "^16.4.1",
    "react-redux": "^5.0.7",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.0",
    "redux-saga": "^0.16.0"
  },
  "devDependencies": {
    "babel-jest": "^23.2.0",
    "chai": "^4.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "jest": "^23.3.0",
    "jest-enzyme": "^6.0.2",
    "jest-plugin-context": "^2.9.0",
    "redux-mock-store": "^1.5.3",
    "regenerator-runtime": "^0.12.0",
    "webpack-dev-server": "2.11.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "<rootDir>/app/javascript/**/*.{js,jsx}"
    ],
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript",
      "spec/javascript"
    ],
    "roots": [
      "<rootDir>/app/javascript",
      "<rootDir>/spec/javascript"
    ],
    "setupFiles": [
      "jest-plugin-context/setup"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/spec/javascript/setup",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
