{
  "name": "todo_demo",
  "private": true,
  "scripts": {
    "cypress:open": "cypress open",
    "cypress:run": "cypress run",
    "jest": "jest"
  },
  "dependencies": {
    "@rails/webpacker": "3.5",
    "cypress": "^3.0.2",
    "redux": "^4.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^23.2.0",
    "jest": "^23.3.0",
    "regenerator-runtime": "^0.12.0",
    "webpack-dev-server": "2.11.2"
  },
  "jest": {
    "verbose": true,
    "moduleFileExtensions": [
      "js",
      "jsx"
    ],
    "moduleDirectories": [
      "node_modules",
      "app/javascript",
      "spec/javascript"
    ],
    "roots": [
      "<rootDir>/app/javascript",
      "<rootDir>/spec/javascript"
    ],
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    }
  }
}
