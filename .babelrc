{
  "presets": [
    ["env", {
      "modules": false,
      "targets": {
        "browsers": "> 1%",
        "uglify": true
      },
      "useBuiltIns": true
    }],
    "react",
    "es2015",
    "stage-0"
  ],

  "plugins": [
    "syntax-dynamic-import",
    "transform-object-rest-spread",
    "transform-es2015-modules-commonjs",
    "transform-regenerator",
    ["transform-class-properties", { "spec": true }]
  ]
}
